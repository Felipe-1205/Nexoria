command /modo <text> <player>:
	permission: staff
	trigger:
		if arg-1 is "pifia" or "critico" or "normal":
			set {modo.%arg-2%} to arg-1
			send "&eModo de %arg-2% ha sido cambiado a %arg-1%." to player
		else:
			send "&eModo inválido. Usa: pifia, critico, o normal." to player	
command /calculadora <text>:
	trigger:
		set {_operacion} to arg-1
		set {_resultado} to 0
		set {_valid} to false
		# Aceptamos números con decimales (con "."), además de enteros
		if {_operacion} matches "[0-9.]+[+-x/][0-9.]+":
			set {_valid} to true
		else:
			send "&eFormato inválido. Usa (número)(+ - x /)(número). Los decimales usan punto (.)." to player
			stop
		# Si la operación contiene "+"
		if {_operacion} contains "+":
			set {_numeros::*} to split {_operacion} at "+"
			set {_resultado} to {_numeros::1} parsed as number + {_numeros::2} parsed as number
		# Si la operación contiene "-"
		else if {_operacion} contains "-":
			set {_numeros::*} to split {_operacion} at "-"
			set {_resultado} to {_numeros::1} parsed as number - {_numeros::2} parsed as number
		# Si la operación contiene "x"
		else if {_operacion} contains "x":
			set {_numeros::*} to split {_operacion} at "x"
			set {_resultado} to {_numeros::1} parsed as number * {_numeros::2} parsed as number
		# Si la operación contiene "/"
		else if {_operacion} contains "/":
			set {_numeros::*} to split {_operacion} at "/"
			# Verificar que no se divida por 0
			if {_numeros::2} parsed as number is 0:
				send "&eError: No se puede dividir por 0." to player
				stop
			set {_resultado} to {_numeros::1} parsed as number / {_numeros::2} parsed as number
		# Enviar el resultado al jugador y a la consola
		send "&aEl resultado de &b%arg-1% &aes &b%{_resultado}%&a." to player
		send "&aEl resultado de &b%arg-1% &aes &b%{_resultado}%&a." to the console

