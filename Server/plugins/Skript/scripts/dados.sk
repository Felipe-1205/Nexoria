command /mjd <number> <text> <number> <text>:
	permission: staff
	usage: "&eError: Uso incorrecto. La sintaxis correcta es: /mjc <Distancia> <nombre el ponga _ para un espacio> <modificador solo si despues pone un atributo si no ponga 0> <algun atributo o formula de #d#>."
	trigger:
		set {_distancia} to arg 1
		set {_nombre} to arg 2
		set {_modificador} to arg 3
		set {_opcional} to arg 3
		replace all "_" with " " in {_nombre}
		if arg 4 is "Fuerza", "Inteligencia", "Destreza", "Sabiduria", "Constitucion" or "Carisma" :
			if {modo.%player%} is "pifia":
				set {_resultado} to 1
			else if {modo.%player%} is "critico":
				set {_resultado} to 20
			else if {modo.%player%} is "normal":
				set {_resultado} to a random integer between 1 and 20
			set {_respuesta} to {_modificador}+{_resultado}
			send "&8[&c%{_nombre}%&8]&fTiro &c&l%arg 4% &fy salió un &c%{_resultado}%+%{_modificador}%: &l%{_respuesta}%&f."  to all players in radius {_distancia} around player
			send "&8[&c%{_nombre}%&8]&fTiro &c&l%arg 4% &fy salió un &c%{_resultado}%+%{_modificador}%: &l%{_respuesta}%&f."  to the console
		else if arg 4 matches "[0-9]+d[0-9]+":
			set {_tiradas::*} to split arg 4 at "d"
			set {_numDados} to {_tiradas::1} parsed as number
			set {_caras} to {_tiradas::2} parsed as number
			if {_numDados} > 100:
				send "&eNo puedes tirar más de 100 dados a la vez." to the player
				stop
			if {_caras} > 100:
				send "&eEl dado no puede tener más de 100 caras." to the player
				stop
			set {_sumaTotal} to 0
			if {modo.%player%} is "pifia":
				set {_sumaTotal} to {_numDados}
			else if {modo.%player%} is "critico":
				set {_sumaTotal} to {_numDados} * {_caras}
			else if {modo.%player%} is "normal":
				loop {_numDados} times:
					set {_resultado} to a random integer between 1 and {_caras}
					add {_resultado} to {_sumaTotal}
			send "&8[&c%{_nombre}%&8]&fTiro &c&l%arg 4% &fy salió un &c&l%{_sumaTotal}%&f."  to all players in radius {_distancia} around player
			send "&8[&c%{_nombre}%&8]&fTiro &c&l%arg 4% &fy salió un &c&l%{_sumaTotal}%&f."  to the console
